@namespace("keybase.1")
protocol avatars {
    import idl "common.avdl";

    @typedef("string") record AvatarUrl {}
    @typedef("string") record AvatarFormat {}

    record LoadUserAvatarsRes {
        map<string, map<AvatarFormat, AvatarUrl>> picmap;
    }

    LoadUserAvatarsRes loadUserAvatars(array<string> names, array<AvatarFormat> formats);
    LoadUserAvatarsRes loadTeamAvatars(array<string> names, array<AvatarFormat> formats);
}